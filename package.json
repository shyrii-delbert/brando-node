{
  "name": "brando",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm run build && node --enable-source-maps ./dist/app.js",
    "build": "rm -rf dist/* && node config/build.js",
    "build_prd": "rm -rf dist/* && NODE_ENV=production node config/build.js",
    "publish_staging": "pnpm run build_prd && PUB_ENV=staging bash ./config/publish.sh",
    "publish_prd": "pnpm run build_prd && PUB_ENV=prd bash ./config/publish.sh",
    "gen-idl": "buf generate"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.1.0",
    "@grpc/grpc-js": "^1.11.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.11.3",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-status-codes": "^2.1.4",
    "lodash-es": "^4.17.21",
    "log4js": "^6.3.0",
    "mariadb": "^2.5.5",
    "qcloud-cos-sts": "^3.0.6",
    "sequelize": "^6.12.0-alpha.1",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.42.0",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.5",
    "@types/uuid": "^8.3.3",
    "esbuild": "^0.13.15",
    "tencentcloud-sdk-nodejs": "^4.0.245"
  }
}
